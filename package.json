{
    "name": "masticate",
    "version": "0.0.8",
    "description": "Chews up HTML, spits out recipes. A library to extract recipe microformats from HTML documents. Inspired by the Ruby GEM hangry.",
    "keywords": [
        "module",
        "recipes"
    ],
    "main": "lib/index.js",
    "scripts": {
        "build": "babel ./src --source-maps-inline -d ./lib",
        "test": "mocha --require @babel/register src/**.spec.js src/**/**.spec.js",
        "prepublish": "npm run build"
    },
    "author": {
        "name": "Troy Stanger",
        "email": "stanger@gmail.com",
        "url": "https://github.com/stanger/masticate/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stanger/masticate.git"
    },
    "bugs": {
        "url": "https://github.com/stanger/masticate/issues"
    },
    "homepage": "https://github.com/stanger/masticate",
    "license": "MIT",
    "dependencies": {
        "@babel/polyfill": "^7.0.0",
        "async": "^2.6.1",
        "cheerio": "^1.0.0-rc.2",
        "iso8601-duration": "^1.1.6",
        "urijs": "^1.19.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "@babel/register": "^7.0.0",
        "chai": "^4.2.0",
        "mocha": "^5.2.0"
    }
}
