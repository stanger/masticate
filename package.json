{
  "name": "masticate",
  "version": "0.0.1",
  "description": "Chews up HTML, spits out recipes. A library to extract recipe microformats from HTML documents. Inspired by the Ruby GEM hangry.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src --source-maps-inline -d ./lib",
    "test": "mocha --require babel-register --require babel-polyfill lib/**.spec.js lib/**/**.spec.js",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Troy Stanger"
  },
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "cheerio": "^1.0.0-rc.2",
    "iso8601-duration": "^1.1.1",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.1"
  }
}
