{
    "name": "masticate",
    "version": "0.1.3",
    "description": "Chews up HTML, spits out recipes. A library to extract recipe microformats from HTML documents. Inspired by the Ruby GEM hangry.",
    "keywords": [
        "module",
        "recipes"
    ],
    "main": "lib/index.js",
    "scripts": {
        "build": "babel ./src -d ./lib",
        "lint": "eslint --fix ./src",
        "test": "mocha --require @babel/register src/**.spec.js src/**/**.spec.js",
        "prepare": "npm run build"
    },
    "author": {
        "name": "Troy Stanger",
        "email": "stanger@gmail.com",
        "url": "https://github.com/stanger/masticate/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stanger/masticate.git"
    },
    "bugs": {
        "url": "https://github.com/stanger/masticate/issues"
    },
    "homepage": "https://github.com/stanger/masticate",
    "license": "MIT",
    "dependencies": {
        "async": "^3.0.0",
        "cheerio": "^1.0.0-rc.10",
        "core-js": "^3.15.2",
        "iso8601-duration": "^1.3.0",
        "regenerator-runtime": "^0.13.8",
        "urijs": "^1.19.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/eslint-parser": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/register": "^7.14.5",
        "chai": "^4.3.4",
        "eslint": "^8.6.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "mocha": "^9.2.2"
    }
}
